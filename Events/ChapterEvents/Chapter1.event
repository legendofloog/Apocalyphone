BeginningScene:
	COUNTER_SET 0 0
	REMOVEPORTRAITS
	BACG 0x23
	FADU 5
	Text(Chapter1BeginningSceneA)
	FADI 5
	CLEAN
	STAL 60
	//ENUT 0xEF //Fixed growths
	LOAD1 0x1 EnemyUnits
	ENUN
	CAMERA_CENTERED [14,1]
	EVBIT_F 2
	FADU 5
	CamOn
	LOAD1 0x0 Units1 //0x0 instead of 0x1 seems to be for allies on maps with prep screens
	ENUN
	CamOff
	CAMERA_CENTERED Trystan
	Text(Chapter1BeginningSceneB1)
	STAL 15
	CAMERA_CENTERED [7,16]
	STAL 45
	CAMERA_CENTERED Trystan
	Text(Chapter1BeginningSceneB2)
	STAL 30
	CamOn
	MOVE 50 Monster_A [7,7]
	MOVE 50 Monster_B [10,9]
	ENUN
	CamOff
	CAMERA_CENTERED Trystan
	Text(Chapter1BeginningSceneB3)
	NoFade
	ENDA

EndingScene:
	MUSCFAST 0x7FFF
	CAMERA_CENTERED Trystan
	Text(Chapter1EndingSceneA)
	SetBackground(0x2)
	COUNTER_CHECK 0
	SVAL s2 2
	//If Counter < 2 goto 1 else
	BLT 1 sC s2
		//If counter < 4 goto 2 else
		SVAL s2 4
		BLT 2 sC s2
			//Perfect score, goto 3
			GOTO 3
	//1 bad score
	LABEL 1
		Text(Chapter1EndingSceneBc)
		GOTO 4
	//2 decent score
	LABEL 2
		Text(Chapter1EndingSceneBb)
		GOTO 4
	//3 perfect score
	LABEL 3
		Text(Chapter1EndingSceneBa)
	LABEL 4
	FADI 5
	MNC2 0
	NoFade
	ENDA

