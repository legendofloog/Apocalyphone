IvaSelect:
	CHECK_ACTIVE
	SVAL s1 Iva
	BEQ 1 s1 sC
	ENUF 0x0B // unset completion flag if unit selected isn't Iva
	GOTO 2
	LABEL 1
	SOUN 0x69
	STAL 20
	CameraCursor2(14,10,90)
	CAMERA_CENTERED Iva
	Text(Chapter1HintText)
	LABEL 2
	NoFade
	ENDA	

